{
  "name": "media-on-demand-webapp",
  "version": "1.0.0",
  "description": "A web app that allows tenants register and configure their media-on-demand presets.",
  "main": "index.js",
  "repository": "git@github.com:MediaSocialNetwork/media-on-demand-webapp.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "0.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "nodemon": "1.12.1",
    "style-loader": "0.19.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "webpack": "3.8.1"
  },
  "scripts": {
    "install:all": "yarn && yarn install:app && yarn install:server && yarn install:dev-server",
    "install:app": "cd app/ && yarn && cd ..",
    "install:server": "cd server/ && yarn && cd ..",
    "install:dev-server": "cd internals/dev-server/ && yarn && cd ../..",
    "dev:build": "webpack --config internals/webpack/webpack.dev.babel.js",
    "dev:server": "NODE_ENV=development node server/index.js"
  }
}
